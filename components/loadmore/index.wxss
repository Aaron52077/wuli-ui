.wuli-loadmore{position:relative;width:65%;margin:21px auto;line-height:20px;font-size:14px;text-align:center;vertical-align:middle}.wuli-loadmore .wuli-loading{margin-right:4px}.wuli-loadmore__tips{display:inline-block;vertical-align:middle;height:20px;line-height:20px}.wuli-loadmore__nodata,.wuli-loadmore__nomore{color:#999}.wuli-loadmore__nodata::after,.wuli-loadmore__nomore::after{content:'';position:absolute;top:0;left:0;width:200%;height:200%;transform:scale(.5);transform-origin:0 0;pointer-events:none;box-sizing:border-box;border:0 solid #e5e5e5;border-top-width:1px}.wuli-loadmore__nodata{margin-top:120px}.wuli-loadmore__nodata .wuli-loadmore__tips{position:relative;top:-11px;background:#f9f9f9;padding:0 6px;z-index:1}.wuli-loadmore__dot{content:"";position:absolute;left:50%;top:10px;margin-left:-2px;margin-top:-2px;width:4px;height:4px;-webkit-border-radius:50%;border-radius:50%;background-color:#e5e5e5;display:inline-block;vertical-align:middle}.wuli-loading{width:20px;height:20px;display:inline-block;vertical-align:middle;animation:weuiLoading 1s steps(12, end) infinite;background:transparent url(data:image/svg+xml;base64,) no-repeat;-webkit-background-size:100%;background-size:100%}